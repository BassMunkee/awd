               </table:table-cell>
               <table:table-cell table:style-name="Table8.A2" office:value-type="float" office:value="4">
                  <text:p text:style-name="Table_20_Contents">4</text:p>
               </table:table-cell>
               <table:table-cell table:style-name="Table8.C2" office:value-type="string">
